{{ form_start(form) }}
<div class="groupes">
    <div class="group">
        {{ form_row(form.email) }}
        {{ form_row(form.password) }}
        {{ form_row(form.firstName) }}
        {{ form_row(form.lastName) }}
    </div>
    <div class="group">
        {{ form_row(form.address) }}
        {{ form_row(form.city) }}
        {{ form_row(form.country) }}
        {{ form_row(form.phone) }}
    </div>

</div>

<button type="submit" class="btn">Save</button>
{{ form_end(form) }}
